const getCar = (carName) => {
    const searchKey = carName.toLowerCase();
    
    // Проверяем существование ключа в объекте cars
    if (cars && cars[searchKey]) {
        console.log(cars[searchKey]);
        return cars[searchKey];
    } else {
        console.log("Авто не найдено");
        return "Авто не найдено";
    }
};
